{
  "name": "Tenable.io",
  "version": "0.0.1",
  "description": "Frontend",
  "private": true,
  "main": "index.js",
  "repository": "ssh://git@stash.corp.tenablesecurity.com:7999/tps/tio_ui.git",
  "workspaces": [
    "apps/*",
    "deps/*"
  ],
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.41",
    "@babel/core": "7.0.0-beta.41",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.41",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.41",
    "@babel/polyfill": "7.0.0-beta.41",
    "@babel/preset-env": "7.0.0-beta.41",
    "@babel/preset-react": "7.0.0-beta.41",
    "@babel/register": "7.0.0-beta.41",
    "ansi-colors": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-react-intl": "2.3.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chai": "^4.1.2",
    "commander": "^2.11.0",
    "connect-history-api-fallback": "^1.4.0",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.4",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-fsa": "1.0.12",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "fancy-log": "^1.3.2",
    "fetch-mock": "^5.13.1",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-exec": "^2.1.3",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "jest": "^21.2.1",
    "jest-junit": "^3.4.1",
    "npm-registry-client": "^8.5.0",
    "plugin-error": "^1.0.1",
    "pre-commit": "^1.2.2",
    "react-intl-translations-manager": "^5.0.1",
    "redux-mock-store": "^1.3.0",
    "require-dir": "^0.3.2",
    "semver": "^5.4.1",
    "semver-regex": "^1.0.0",
    "stats-webpack-plugin": "^0.6.2",
    "svg-url-loader": "^2.3.2",
    "through2": "^2.0.3",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^1.0.1",
    "wdio-mocha-framework": "^0.5.12",
    "wdio-sauce-service": "^0.4.8",
    "wdio-junit-reporter": "^0.4.3",
    "wdio-selenium-standalone-service": "^0.0.9",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^4.10.2",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-stream": "^4.0.0",
    "yargs": "^9.0.1"
  },
  "jest": {
    "setupFiles": [
      "./jest/__mocks__/storage.js",
      "./jest/__setup__/enzyme.js",
      "./jest/__setup__/app-globals.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__setup__/assetsTransformer.js",
      "^i18n\\/(.*)": "<rootDir>/deps/alloy/i18n/$1",
      "^tio-app\\/(.*)": "<rootDir>/apps/tenableio/src/$1",
      "^tio-container-security\\/(.*)": "<rootDir>/apps/container-security/src/$1",
      "^tio-dashboards\\/(.*)": "<rootDir>/apps/dashboards/src/$1",
      "^tio-settings\\/(.*)": "<rootDir>/apps/settings/src/$1",
      "^tio-vm\\/(.*)": "<rootDir>/apps/vulnerability-management/src/$1",
      "^tio-lumin\\/(.*)": "<rootDir>/apps/lumin/src/$1",
      "^schedule\\/(.*)": "<rootDir>/deps/common/src/schedule/$1",
      "^validation\\/(.*)": "<rootDir>/deps/common/src/validation/$1"
    },
    "testResultsProcessor": "jest-junit",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/gulpfile"
    ]
  },
  "scripts": {
    "init": "yarn install --skip-integrity-check",
    "dockerInit": "./folgers/init",
    "dockerDev": "./folgers/launch",
    "deploy": "./folgers/deploy",
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development gulp",
    "build": "cross-env BABEL_ENV=production NODE_ENV=production gulp build",
    "test": "gulp test",
    "testless": "yarn run dev --skip-tests",
    "lint": "gulp lint",
    "lint-autofix": "./node_modules/.bin/eslint --fix",
    "clean": "gulp clean",
    "translations": "./folgers/translations-coverage",
    "sauce": "wdio wdio.conf.sauce.js",
    "locale2e": "wdio wdio.conf.local.js",
    "precommit-entrypoint": "cross-env ./folgers/precommit/entrypoint.js",
    "precommit-lint": "cross-env ./folgers/precommit/lint"
  },
  "precommit": {
    "silent": true,
    "run": "precommit-entrypoint"
  }
}
